<!DOCTYPE html>
<html ng-app="todoApp">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>

        <meta name="viewport" content="width=device-width, initial-scale=1">

        @Styles.Render("~/Content/css")
    </head>
    <body>
        <div class="container">
            @RenderBody()
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/Scripts/vendor")
        @Scripts.Render("~/Scripts/app")
    </body>
</html>
